   "source": [
    "Event = networking.index.tolist() #creating a list of indexes to reference the events.\n",
    "\n",
    "classes = schedules.index.tolist()\n",
    "delete_index = []  #creating an empty list to store the indexes of the events to delete\n",
    "\n",
    "#Defining a function to get rid of the events that clash with scheduled classes. \n",
    "def schedule_clashes():\n",
    "        for e in Event:\n",
    "            for f in classes:\n",
    "                if networking.loc[e,\"Date\"] == schedules.loc[f,\"Date\"] and networking.loc[e, \"Start time\"] >= schedules.loc[f, \"Start Time\"] and networking.loc[e, \"Start time\"] <= schedules.loc[f,\"End Time\"] or networking.loc[e,\"Date\"] == schedules.loc[f,\"Date\"] and networking.loc[e, \"End time\"] >= schedules.loc[f, \"Start Time\"] and networking.loc[e, \"End time\"] <= schedules.loc[f,\"End Time\"] or networking.loc[e,\"Date\"] == schedules.loc[f,\"Date\"] and networking.loc[e, \"Start time\"] <= schedules.loc[f, \"Start Time\"] and networking.loc[e, \"End time\"] >= schedules.loc[f,\"End Time\"] :\n",
    "                    delete_index.append(e)\n",
    "        networking.drop(index = delete_index, inplace = True)\n",
    "        \n",
    "schedule_clashes()"
