    "#for loop to multiply utility (number of people) by the interest factor (preference by category) and calculate total utility\n",
    "Event = networking.index.tolist()\n",
    "total_utility = []\n",
    "\n",
    "def preferences_function():\n",
    "    for e in Event:\n",
    "        if networking.Category[e] == \"Consulting\":\n",
    "            total_utility.append(networking.iloc[e,7] * preferences.iloc[0,2])\n",
    "        elif networking.Category[e] == \"Technology\":\n",
    "            total_utility.append(networking.iloc[e,7] * preferences.iloc[0,3])\n",
    "        elif networking.Category[e] == \"Finance\":\n",
    "            total_utility.append(networking.iloc[e,7] * preferences.iloc[0,4])\n",
    "        elif networking.Category[e] == \"Data Science\":\n",
    "            total_utility.append(networking.iloc[e,7] * preferences.iloc[0,5])\n",
    "        elif networking.Category[e] == \"Marketing\":\n",
    "            total_utility.append(networking.iloc[e,7] * preferences.iloc[0,6])\n",
    "        elif networking.Category[e] == \"Healthcare\":\n",
    "            total_utility.append(networking.iloc[e,7] * preferences.iloc[0,7])\n",
    "        elif networking.Category[e] == \"Entrepreneurship\":\n",
    "            total_utility.append(networking.iloc[e,7] * preferences.iloc[0,8])\n",
    "\n",
    "preferences_function()\n",
    "\n",
    "networking['Total Utility'] = total_utility"
